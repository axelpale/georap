{
  "name": "tresdb",
  "private": true,
  "version": "12.0.0-alpha",
  "description": "Geographical CMS built on Express.js",
  "keywords": [
    "GIS",
    "CMS",
    "maps"
  ],
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/axelpale/tresdb.git"
  },
  "scripts": {
    "start": "npm run server:production",
    "watchstart": "nodemon --watch local_modules --watch server --ext js,ejs,json --exec 'npm run lint:server && npm run server:production'",
    "build": "webpack client",
    "watchbuild": "nodemon --watch client --watch local_modules --ext js,ejs,css,json --exec 'npm run lint:client && npm run build'",
    "server:development": "NODE_ENV=development node server/index.js",
    "server:production": "NODE_ENV=production node server/index.js",
    "server:test": "NODE_ENV=test node server/index.js",
    "migrate": "node cli/migrate.js",
    "migrate:example": "node migration/loadExample.js",
    "test": "npm run lint && npm run test:parsekml",
    "test:api:markers": "NODE_ENV=test mocha specs/api/markers.spec.js",
    "test:migration": "NODE_ENV=test mocha migration/*.spec.js",
    "test:migration:iter": "NODE_ENV=test mocha migration/iter.spec.js",
    "test:parsekml": "NODE_ENV=test mocha server/api/locations/importer/parsekml/parsekml.spec.js",
    "lint": "npm run lint:client && npm run lint:server && npm run lint:others",
    "lint:config": "eslint \"config/**/*.js\" && echo 'OK'",
    "lint:client": "eslint \"client/**/*.js\" && echo 'OK'",
    "lint:server": "eslint \"server/**/*.js\" \"worker/**/*.js\" && echo 'OK'",
    "lint:migration": "eslint \"migration/**/*.js\" && echo 'OK'",
    "lint:others": "eslint \"cli/**/*.js\" && echo 'OK'",
    "mongo": "mongo tresdb",
    "mongo:test": "mongo test",
    "mongod:init": "mkdir -p .data/db && mongod --dbpath=.data/db",
    "mongod:init:users": "mongo config/mongo.init.js",
    "mongod": "mongod --auth --dbpath=.data/db",
    "reset": "rm -rf .data",
    "worker": "node worker/index.js"
  },
  "author": "Akseli Pal√©n <akseli.palen@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.1.13",
    "ajv": "^6.12.6",
    "async": "^2.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^3.4.1",
    "camaro": "^6.0.4",
    "commander": "^2.9.0",
    "component-emitter": "^1.2.1",
    "distfilter": "file:local_modules/distfilter",
    "download": "^7.1.0",
    "ejs": "^2.5.2",
    "ejs-loader": "^0.3.0",
    "email-validator": "^1.0.7",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "extract-zip": "^1.6.5",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "http-status-codes": "^1.1.6",
    "jsonwebtoken": "^8.0.1",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.17.11",
    "mime": "^2.0.1",
    "moment": "^2.15.1",
    "morgan": "^1.7.0",
    "multer": "^1.2.1",
    "nodemailer": "^4.1.0",
    "page": "^1.7.1",
    "pretty-events": "file:local_modules/pretty-events",
    "proj4": "^2.4.3",
    "purify-markdown": "file:local_modules/purify-markdown",
    "qs": "^6.3.1",
    "query-string": "^5.1.1",
    "request": "^2.88.2",
    "sanitize-filename": "^1.6.1",
    "sax": "^1.2.4",
    "semver": "^7.3.1",
    "sharp": "^0.26.1",
    "shortid": "^2.2.15",
    "slugify": "^1.1.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "striptags": "^3.1.0",
    "timestamp": "file:local_modules/timestamp",
    "tresdb-cap": "file:local_modules/tresdb-cap",
    "tresdb-config": "file:local_modules/tresdb-config",
    "tresdb-db": "file:local_modules/tresdb-db",
    "tresdb-key": "file:local_modules/tresdb-key",
    "tresdb-models": "file:local_modules/tresdb-models",
    "tresdb-points": "file:local_modules/tresdb-points",
    "tresdb-ui": "file:local_modules/tresdb-ui",
    "tresdb-urls": "file:local_modules/tresdb-urls",
    "turndown": "^7.0.0",
    "url-join": "^4.0.1",
    "zpad": "^0.5.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-eslint": "^10.1.0",
    "clone": "^2.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "mocha": "^6.1.4",
    "nodemon": "^2.0.2",
    "should": "^13.2.3",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
