{
  "name": "tresdb",
  "private": true,
  "version": "4.1.0",
  "description": "Geographical CMS built on Express.js",
  "keywords": [
    "GIS",
    "CMS"
  ],
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/axelpale/tresdb.git"
  },
  "scripts": {
    "start": "npm run lint && npm run server:production",
    "server:development": "NODE_ENV=development node server/index.js",
    "server:production": "NODE_ENV=production node server/index.js",
    "server:test": "NODE_ENV=test node server/index.js",
    "loadsample": "node cli/loadsample.js",
    "migrate": "node cli/migrate.js",
    "backup": "node cli/backup.js",
    "restore": "node cli/restore.js",
    "cluster": "node cli/cluster.js",
    "test": "npm run test:server && npm run test:client && npm run test:migration && npm run test:clustering",
    "test:server": "npm run test:server:models && npm run test:server:handlers",
    "test:server:handlers": "mocha server/handlers/**.spec.js",
    "test:server:models": "mocha server/models/**.spec.js",
    "test:migration": "mocha migration/*.spec.js",
    "test:clustering": "mocha server/services/clustering/lib/specs/*.spec.js",
    "test:client": "casperjs test specs/client/test-login.js",
    "lint": "eslint --config ./config/eslint.js . && echo 'Lint successful.'",
    "mongo": "mongo tresdb",
    "mongo:test": "mongo test",
    "mongod": "mkdir -p .data/db && mkdir -p .data/backups && mongod --auth --dbpath=.data/db",
    "reset": "rm -rf .data"
  },
  "author": "Akseli Pal√©n <akseli.palen@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "ajv": "^4.9.0",
    "async": "^2.1.1",
    "bcryptjs": "^2.3.0",
    "commander": "^2.9.0",
    "component-emitter": "^1.2.1",
    "css-loader": "^0.25.0",
    "ejs": "^2.5.2",
    "ejs-loader": "^0.3.0",
    "email-validator": "^1.0.7",
    "express": "^4.14.0",
    "extend": "^3.0.0",
    "file-loader": "^0.9.0",
    "jsonwebtoken": "^7.1.9",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.15.0",
    "marked": "^0.3.6",
    "mime": "^1.3.4",
    "moment": "^2.15.1",
    "mongodb": "^2.2.12",
    "mongodb-backup": "^1.6.7",
    "mongodb-restore": "^1.6.0",
    "morgan": "^1.7.0",
    "nodemailer": "^2.6.0",
    "page": "^1.7.1",
    "query-string": "^4.2.3",
    "semver": "^5.3.0",
    "shortid": "^2.2.6",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "style-loader": "^0.13.1",
    "url-join": "^1.1.0",
    "webpack": "^1.13.2",
    "zpad": "^0.5.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "casperjs": "^1.1.3",
    "clone": "^2.1.0",
    "eslint": "^3.4.0",
    "mocha": "^3.1.0",
    "phantomjs-prebuilt": "^2.1.12",
    "should": "^11.1.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.15.1"
  }
}
