{
  "name": "tresdb",
  "private": true,
  "version": "8.0.0-alpha.3",
  "description": "Geographical CMS built on Express.js",
  "keywords": [
    "GIS",
    "CMS"
  ],
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/axelpale/tresdb.git"
  },
  "scripts": {
    "start": "npm run lint && npm run server:production",
    "server:development": "NODE_ENV=development node server/index.js",
    "server:production": "NODE_ENV=production node server/index.js",
    "server:test": "NODE_ENV=test node server/index.js",
    "loadsample": "node cli/loadsample.js",
    "ensureindices": "node cli/ensureindices.js",
    "migrate": "node cli/migrate.js",
    "backup": "node cli/backup.js",
    "restore": "node cli/restore.js",
    "test": "npm run test:migration",
    "test:api:markers": "mocha server/api/markers/dal.spec.js",
    "test:migration": "mocha migration/*.spec.js",
    "test:migration:iter": "mocha migration/iter.spec.js",
    "lint": "eslint . && echo 'Lint successful.'",
    "mongo": "mongo tresdb",
    "mongo:test": "mongo test",
    "mongod": "mkdir -p .data/db && mkdir -p .data/backups && mongod --auth --dbpath=.data/db",
    "reset": "rm -rf .data",
    "worker": "node worker/index.js"
  },
  "author": "Akseli Pal√©n <akseli.palen@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@google/maps": "^0.4.3",
    "async": "^2.1.1",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.16.0",
    "commander": "^2.9.0",
    "component-emitter": "^1.2.1",
    "css-loader": "^0.28.7",
    "ejs": "^2.5.2",
    "ejs-loader": "^0.3.0",
    "email-validator": "^1.0.7",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "file-loader": "^0.11.2",
    "fs-extra": "^4.0.2",
    "http-status-codes": "^1.1.6",
    "jsonwebtoken": "^8.0.1",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.15.0",
    "marked": "^0.3.6",
    "mime": "^2.0.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.1",
    "mongodb": "^2.2.24",
    "mongodb-backup": "^1.6.9",
    "mongodb-restore": "^1.6.2",
    "morgan": "^1.7.0",
    "multer": "^1.2.1",
    "nodemailer": "^4.1.0",
    "page": "^1.7.1",
    "proj4": "^2.4.3",
    "qs": "^6.3.1",
    "query-string": "^5.0.0",
    "sanitize-filename": "^1.6.1",
    "semver": "^5.3.0",
    "sharp": "^0.18.3",
    "shortid": "^2.2.6",
    "slugify": "^1.1.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "style-loader": "^0.18.2",
    "url-join": "^2.0.2",
    "webpack": "^3.5.6",
    "zpad": "^0.5.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "clone": "^2.1.0",
    "eslint": "^4.6.1",
    "mocha": "^3.1.0",
    "should": "^11.1.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^2.8.1"
  }
}
