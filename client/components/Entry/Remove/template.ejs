<div class="panel panel-default">
  <div class="panel-body">
    <p class="text-warning"><%= info %></p>
    <p class="text-warning"><%= __('are-you-sure-cannot-undo') %></p>
    <div class="form-remove-confirmation">
      <button type="button" class="form-cancel-btn btn btn-default"><%= __('cancel') %></button>
      <button type="button" class="form-remove-btn btn btn-danger"><%= __('delete-ok') %></button>
    </div>

    <div class="form-remove-progress progress hidden">
      <div class="progress-bar progress-bar-striped active" role="progressbar" style="width: 100%">
        <span class="sr-only">Waiting for server</span>
      </div>
    </div>
  </div>
</div>
