<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">

      <h1><span class="glyphicon glyphicon-user" aria-hidden="true"></span> <%= username %></h1>

      <div id="georap-user-loading">
        <div class="progress">
          <div class="progress-bar progress-bar-striped active" role="progressbar" style="width: 100%">
            <span class="sr-only">Waiting for server</span>
          </div>
        </div>
      </div>

      <div id="georap-user-points" style="margin-top: 1.62em"></div>

      <div style="margin-top: 2.62em">
        <h3><%= __('event-log') %></h3>
        <div id="georap-user-events"></div>
      </div>

    </div>
  </div>
</div>
