<% flags.forEach(function (flag) { %>
<h3><span class="glyphicon glyphicon-<%= flag.glyphicon %>" aria-hidden="true"></span> <%= flag.count %> <%= flag.plural %></h3>
<% }) %>
<h3><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span> <%= adds %> added locations</h3>
<h3><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> <%= posts %> posts</h3>
<h3><span class="glyphicon glyphicon-tags" aria-hidden="true"></span> <%= classifications %> tags</h3>
<h3><span class="glyphicon glyphicon-comment" aria-hidden="true"></span> <%= comments %> comments</h3>
<div style="height: 0.62em; margin-top: 0.38em; border-bottom: 3px double black;"></div>
<h3>= <%= points %> <span class="glyphicon glyphicon-star" aria-hidden="false"></span></h3>
<p>The points are updated hourly.</p>
