<% var locName = location.getName(); %>
<% locName = (locName === '' ? __('untitled') : locName); %>
<h1>
  <span class="location-name-display"><%= locName %></span>
  <% if (able('locations-update')) { %>
  <button type="button" class="location-rename-show btn btn-default btn-sm">
    <span class="glyphicon glyphicon-pencil"></span>
  </button>
  <% } %>
</h1>

<div class="location-rename-form-container"></div>
