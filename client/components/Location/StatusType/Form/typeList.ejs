<%
  locationTypes.forEach(function (t) {
    var isActive = (t === currentType);
    var label = __('location-type', t);
%>
  <button data-type="<%= t %>" title="<%= label %>" class="location-type-<%= t %> georap-tag <%= isActive ? 'georap-tag-active' : '' %>">
    <img src="<%= toSymbolUrl(t) %>" width="22" height="40">
    <span class="type-label"><%= label %></span>
  </button>
<%
  });
%>
