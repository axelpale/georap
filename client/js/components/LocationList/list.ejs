
<% if (markers.length === 0) { %>
  <div>no matches</div>
<% } %>

<ul class="list-group">
<% markers.forEach(function (m) { %>
  <li class="list-group-item">
    <div class="pull-right">
      <%= tagsTemplate({ tags: m.tags }) %>
    </div>
    <a href="/locations/<%= m._id %>"><%= m.name %></a>
  </li>
<% }); %>
</ul>
