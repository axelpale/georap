<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">

      <h1><span class="glyphicon glyphicon-import" aria-hidden="true"></span> Results</h1>

      <p><strong>Batch number:</strong> <%= batchId %></p>

      <p id="tresdb-outcome-message"></p>

      <div id="tresdb-outcome-progress" class="progress">
        <div class="progress-bar progress-bar-striped active" role="progressbar" style="width: 100%">
          <span class="sr-only">Waiting for server</span>
        </div>
      </div>

      <div id="tresdb-outcome-list" class="hidden"></div>

      <form id="tresdb-outcome-form" class="hidden" style="margin-bottom: 5em">
        <button id="tresdb-outcome-cancel" type="button" class="btn btn-default">Discard duplicates</button>
        <div class="pull-right">
          <button type="submit" class="btn btn-primary">Merge duplicates</button>
        </div>
      </form>

    </div>
  </div>
</div>
