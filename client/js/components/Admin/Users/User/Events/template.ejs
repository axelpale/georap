<h3>Membership history</h3>
<table class="table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Author</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
  <% events.forEach(function (ev) { %>
    <tr>
      <td><%= ev.time %></td>
      <td><a href="/admin/users/<%= ev.user %>"><%= ev.user %></a></td>
      <td><%= ev.type %></td>
    </tr>
  <% }); %>
  </tbody>
</table>
