<div id="tresdb-viewon-container" class="hidden">

  <div class="panel panel-primary">
    <div class="panel-heading">
      Maps
    </div>
    <div class="panel-body">

      <p>
        <h4>View the same coordinates on:</h4>
        <% services.forEach(function (serv) { %>
        <a href="<%= serv.url %>" class="btn btn-default" role="button" target="_blank"><%= serv.name %></a>
        <% }); %>
      </p>

      <div class="pull-right">
        <button id="tresdb-viewon-cancel" type="button" class="btn btn-default">Cancel</button>
      </div>

    </div>
  </div>

</div>
