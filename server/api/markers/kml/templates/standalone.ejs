<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
<Folder>
  <name><%= siteTitle %></name>
  <% markers.forEach(function (m) { %>
  <Placemark>
    <name><%= m.name %></name>
    <description><%= m.status %> <%= m.type %> by <%= m.user %></description>
    <Point>
      <coordinates><%= m.geom.coordinates.join(',') %></coordinates>
    </Point>
  </Placemark>
  <% }); %>
</Folder>
</kml>
